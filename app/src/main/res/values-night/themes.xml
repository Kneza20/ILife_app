<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Ilife_app_design" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_200</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="SuperTheme">
        <item name="android:textColor">@color/white</item>
        <item name="android:backgroundTint">@color/nardo_black</item>
    </style>
    <style name="SCREEN">
        <item name="android:background">@drawable/restaurant</item>
    </style>
    <style name="TEXT">
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="TVBG">
        <item name="android:backgroundTint">@color/nardo_black</item>
    </style>
    <style name="PROF_BG">
        <item name="android:background">@drawable/profile_tv_bg_dark</item>
    </style>
    <style name="IMG_EMAIL">
        <item name="android:background">@drawable/envelopedark</item>
    </style>
    <style name="IMG_PHONE">
        <item name="android:background">@drawable/telephonedark</item>
    </style>

    <style name="IMG_DASHBOARD">
        <item name="android:background">@drawable/layoutdark</item>
    </style>

    <style name="IMG_PAYMENT">
        <item name="android:background">@drawable/walletdark</item>
    </style>

    <style name="IMG_FAQ">
        <item name="android:background">@drawable/infodark</item>
    </style>

    <style name="ICON">
        <item name="android:background">@drawable/logo_white</item>
    </style>

    <style name="IMG_SIGNOUT">
        <item name="android:background">@drawable/signoutdark</item>
    </style>

    <style name="USER_PROF_BG">
        <item name="android:background">@drawable/dark_bg</item>
    </style>

    <style name="HEADER_ROW">
        <item name="android:background">@color/header_color</item>
    </style>

    <style name="REG_ROW">
        <item name="android:background">@color/regular_row</item>
    </style>
</resources>